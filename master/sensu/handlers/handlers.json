{
  "handlers": {
    "default": {
     "type": "set",
        "handlers": ["file", "influxdb"]
    },
    "file": {
      "type": "pipe",
      "command": "/etc/sensu/plugins/event-file.rb"
    },
    "influxdb": {
      "type": "udp",
      "mutator": "influxdb_line_protocol",
      "socket": {
        "host": "influxdb",
        "port": 8089
      }
    },
    "transpq": {
      "type": "transport",
      "pipe": {
        "type": "fanout",
        "name": "metrics",
        "options": {"durable": true}
      }
    }

  }
}
